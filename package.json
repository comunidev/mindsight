{
  "name": "mindinsight",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "chii start -p 8080 & astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "test": "vitest",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/solid-js": "^3.0.1",
    "@solid-devtools/overlay": "^0.27.7",
    "@solid-primitives/context": "^0.2.1",
    "@solid-primitives/event-listener": "^2.3.0",
    "@solid-primitives/mouse": "^2.0.16",
    "@solidjs-use/rxjs": "^2.3.0",
    "astro": "^3.1.4",
    "csstype": "^3.1.2",
    "dexie": "^3.2.4",
    "fake-indexeddb": "^4.0.2",
    "lodash": "^4.17.21",
    "resolve-cwd": "^3.0.0",
    "rxdb": "^14.17.1",
    "rxjs": "^7.8.1",
    "solid-dexie": "^0.0.5",
    "solid-js": "^1.4.3",
    "solidjs-use": "^2.3.0",
    "ulid": "^2.3.0",
    "utility-types": "^3.10.0",
    "vite": "^4.5.2"
  },
  "devDependencies": {
    "@solid-devtools/debugger": "^0.22.4",
    "@solid-devtools/logger": "^0.7.4",
    "@types/node": "^20.7.0",
    "@types/lodash": "^4.14.199",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@unocss/postcss": "^0.56.4",
    "@unocss/reset": "^0.56.2",
    "chii": "^1.9.0",
    "eslint": "^8.50.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "solid-devtools": "^0.27.7",
    "typescript": "^5.2.2",
    "unocss": "^0.56.2",
    "vitest": "^0.34.5"
  },
  "wallaby": {
    "files": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.js",
      "src/**/*.jsx",
      "src/**/*.astro",
      "!src/**/*.test.ts",
      "!src/**/*.test.tsx",
      "!src/**/*.test.js",
      "!src/**/*.test.jsx"
    ],
    "tests": [
      "src/**/*.test.ts",
      "src/**/*.test.tsx",
      "src/**/*.test.js",
      "src/**/*.test.jsx"
    ]
  },
  "prettier": {
    "printWidth": 90,
    "tabWidth": 2,
    "semi": false,
    "arrowParens": "avoid",
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
